<?xml version="1.0"?>
<SystemConfig xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Attachment>
    <Path>uploads\\users</Path>
    <Directory>yyyyMM</Directory>
    <ImageFormat>jpg|jpeg|bmp|gif|png</ImageFormat>
    <VideoFormat>avi</VideoFormat>
    <AudioFormat>mp3|mp4</AudioFormat>
    <FlashFormat>swf|fla</FlashFormat>
    <AttachmentFormat>doc|txt|rar|zip</AttachmentFormat>
    <HasWaterMark>1</HasWaterMark>
    <HasAbbrImage>1</HasAbbrImage>
    <HasAbbrImageWaterMark>0</HasAbbrImageWaterMark>
    <AbbrImageWidth>300</AbbrImageWidth>
    <AbbrImageHeight>0</AbbrImageHeight>
    <WaterImage>
      <WaterPic>0</WaterPic>
      <WaterCharater>www.dtcms.com</WaterCharater>
      <WaterPicPath>/uploads/users/201001/20100103115850703_abbr.jpg</WaterPicPath>
      <XPercent>0.72</XPercent>
      <YPercent>0.63</YPercent>
      <CharColor>#00bff3</CharColor>
      <Transparence>1</Transparence>
      <FontFamilyName>华文行楷</FontFamilyName>
      <FontSize>18</FontSize>
    </WaterImage>
  </Attachment>
  <SysInfoParam>
    <SiteName>DTCMS内容管理系统</SiteName>
    <SiteDomain>http://www.91aspx.com</SiteDomain>
    <HomeTemplet>/{@dirTemplet}/Content/index.html</HomeTemplet>
    <HomeRule>index.html</HomeRule>
    <HomeUrl>http://www.91aspx.com/index.html</HomeUrl>
    <HomeName>首页</HomeName>
    <ClassPath>{#cmsPath}/archive/</ClassPath>
    <IndexTemplet>/{#Templet}/article/index.html</IndexTemplet>
    <ListTemplet>/{#Templet}/article/list.html</ListTemplet>
    <ArchiveTemplet>/{#Templet}/article/article.html</ArchiveTemplet>
    <IndexRule>{#ClassPath}/index.html</IndexRule>
    <ListRule>{#ClassPath}/list_{#page}.html</ListRule>
    <ArchiveRule>{#ClassPath}/{#DateTime Format=yyyyMM}/{#AID}.html</ArchiveRule>
    <MetaKey>电脑编程网,打造最全的电脑编程学习网站</MetaKey>
    <MetaDescription>web开发,javascript,xml</MetaDescription>
    <SiteCopyRightInfo>DTCMS V1.0</SiteCopyRightInfo>
    <RecordNo>闽ICP备08003476号</RecordNo>
    <AdminEmail>admin@91aspx.com</AdminEmail>
    <DefaultTemplet>default</DefaultTemplet>
    <CheckLevel>1</CheckLevel>
    <LoginValidate>0</LoginValidate>
  </SysInfoParam>
</SystemConfig>